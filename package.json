    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "fusion",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-not-found",
            "@oclif/plugin-autocomplete"
        ]
    },
    "repository": "equinor/fusion-cli",
    "scripts": {
        "copy": "cp -r src/templates/ lib/ && cp -r src/start-app/dist lib/start-app/",
        "postpack": "rm oclif.manifest.json",
        "prepack": "rm -rf lib && tsc -b && npm run copy && oclif-dev manifest",
        "build": "webpack --config ./src/start-app/webpack.config.js",
        "update-templates": "npm run update-templates:dependencies && npm run update-templates:files",
        "update-templates:dependencies": "ts-node src/scripts/update-template-dependencies.ts",
        "update-templates:files": "ts-node src/scripts/update-template-files.ts"
    },
    "types": "lib/index.d.ts",
    "publishConfig": {
        "access": "public"
    },
    "resolutions": {
        "@types/react": "^17.0.71",
        "@types/react-dom": "^17.0.9"
