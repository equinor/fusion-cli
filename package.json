        "@oclif/plugin-not-found": "^1.2.4",
        "@types/execa": "^2.0.0",
        "@types/inquirer": "^8.1.1",
        "@types/listr": "^0.14.4",
        "archiver": "^5.3.0",
        "babel-loader": "^8.2.2",
        "chalk": "^4.1.2",
        "clean-webpack-plugin": "^4.0.0",
        "compression": "^1.7.4",
        "css-loader": "^3.4.2",
        "eslint": "^7.32.0",
        "execa": "^5.1.1",
        "expose-loader": "^3.0.0",
        "express": "^4.17.1",
        "figlet": "^1.5.2",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^6.3.3",
        "get-port": "^5.1.1",
        "history": "^4.9.0",
        "inquirer": "^8.1.2",
        "less": "^3.9.0",
        "less-loader": "^6.0.0",
        "listr": "^0.14.3",
        "log-symbols": "^4.1.0",
        "ncp": "^2.0.0",
        "open": "^8.2.1",
        "path-browserify": "^1.0.1",
        "pkg-install": "^1.0.0",
        "prettier": "^2.4.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router": "^5.2.1",
