    "name": "@equinor/fusion-cli",
    "description": "A cli for creating, starting, building, deploying and publishing Fusion apps and tiles",
    "version": "2.2.4",
    "author": "Fusion Core",
    "bin": {
        "fusion": "bin/run"
    },
    "bugs": "https://github.com/equinor/fusion-cli/issues",
