        "rimraf": "^3.0.2",
        "slugify": "^1.6.0",
        "style-loader": "^1.1.3",
        "ts-loader": "^9.2.5",
        "tslib": "^2.3.1",
        "typesafe-actions": "^5.1.0",
        "typescript": "^4.5.4",
