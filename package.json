        "@hot-loader/react-dom": "^17.0.1",
        "@oclif/command": "^1.5.13",
        "@oclif/config": "^1.17.0",
        "@oclif/plugin-autocomplete": "^0.2.0",
        "@oclif/plugin-help": "^3.0.0",
        "@oclif/plugin-not-found": "^1.2.2",
        "@types/execa": "^2.0.0",
        "@types/inquirer": "^6.0.2",
        "@types/listr": "^0.14.0",
        "archiver": "^4.0.1",
        "babel-loader": "^8.2.2",
        "chalk": "^4.0.0",
        "clean-webpack-plugin": "^3.0.0",
        "compression": "^1.7.4",
        "css-loader": "^3.4.2",
        "execa": "^4.0.0",
        "expose-loader": "^0.7.5",
        "express": "^4.17.0",
        "figlet": "^1.2.1",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^5.0.5",
        "get-port": "^5.0.0",
        "globby": "^11.0.0",
        "history": "^4.9.0",
        "inquirer": "^7.0.4",
        "less": "^3.9.0",
        "less-loader": "^6.0.0",
        "listr": "^0.14.3",
        "log-symbols": "^4.0.0",
        "ncp": "^2.0.0",
        "open": "^7.0.2",
        "path-browserify": "^1.0.1",
        "pkg-install": "^1.0.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "rimraf": "^3.0.2",
        "slugify": "^1.3.4",
        "style-loader": "^1.1.3",
        "ts-loader": "^8.0.14",
        "tslib": "^2.0.1",
        "typesafe-actions": "^5.1.0",
        "typescript": "^4.1.3",
        "webpack": "^5.13.0",
        "webpack-cli": "^4.3.1",
        "webpack-dev-middleware": "^3.7.0",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-merge": "^5.7.3"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.22.0",
        "@oclif/test": "^1.2.4",
        "@oclif/tslint": "^3.1.1",
        "@types/archiver": "^3.0.0",
        "@types/chai": "^4.1.7",
        "@types/chalk": "^2.2.0",
        "@types/compression": "1.7.0",
        "@types/express": "^4.16.1",
        "@types/guid": "^1.0.0",
        "@types/log-symbols": "^3.0.0",
        "@types/mocha": "^8.0.0",
        "@types/node": "^14.0.5",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-router-dom": "^5.1.7",
        "@types/rimraf": "^3.0.0",
        "@types/webpack-dev-middleware": "^3.7.0",
        "@types/webpack-env": "^1.13.9",
        "@types/webpack-hot-middleware": "^2.16.5",
        "@types/webpack-merge": "^4.1.5",
        "chai": "^4.2.0",
        "mocha": "^8.0.1",
        "nyc": "^15.0.0",
        "react-hot-loader": "^4.13.0",
        "source-map-loader": "^1.0.0",
        "ts-node": "^9.1.1",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/equinor/fusion-cli",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "fusion",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-not-found",
            "@oclif/plugin-autocomplete"
        ]
    },
    "repository": "equinor/fusion-cli",
    "scripts": {
        "copy": "cp -r src/templates/ lib/ && cp -r src/start-app/dist lib/start-app/",
        "postpack": "rm oclif.manifest.json",
        "prepack": "rm -rf lib && tsc -b && npm run copy && oclif-dev manifest",
        "build": "webpack --config ./src/start-app/webpack.config.js",
        "update-templates": "npm run update-templates:dependencies && npm run update-templates:files",
        "update-templates:dependencies": "ts-node src/scripts/update-template-dependencies.ts",
        "update-templates:files": "ts-node src/scripts/update-template-files.ts"
    },
    "types": "lib/index.d.ts",
    "publishConfig": {
        "access": "public"
    }
}
